		parsing/pars_quote_utils \
		parsing/pars_t_cmd \
		parsing/pars_t_delemiteur \
		parsing/pars_t_var_init \
		parsing/parsing_lst_add \
		parsing/parsing_lst_add_more \
		parsing/free_exit_token \
